{
  "name": "cdk8s-test",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "cdk8s synth",
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "build": "npm run build-containers && npm run create-netdata-config && npm run update-teslamate-auth && npm run compile && npm run synth",
    "build-containers": "npm run build-node-red",
    "build-node-red": "./charts/homeassistant/node-red/build.sh",
    "create-netdata-config": "(cd charts/netdata; [ -e netdata.conf ] || sed '/^\\s*#/d' netdata.conf.example > netdata.conf)",
    "update-teslamate-auth": "node charts/teslamate/generateAuth.js",
    "apply": "for file in dist/*; do kubectl apply -f $file; done",
    "deploy": "npm run build && npm run apply",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next"
  },
  "dependencies": {
    "cdk8s": "^1.0.0-beta.11",
    "cdk8s-plus": "^0.33.0",
    "cdk8s-plus-17": "^1.0.0-beta.11",
    "constructs": "^3.3.75",
    "pass": "^0.2.0",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "cdk8s-cli": "^1.0.0-beta.11",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  }
}
